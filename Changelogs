-- // This will be edited any time an update occurs [CANNOT BE MODIFIED!].

local function ShowStatus(Txt,Date)
    local Texts = {
       ["Operational"] = "rgb(94,255,94)",
       ["Unoperational"] = "rgb(255,0,43)",
       ["Updating Soon"] = "rgb(255,140,0)",
    };
    return string.format('<font color="%s">%s</font> - Last Update: %s',Texts[Txt], Txt,Date);
end

-- // Variables \\ --
local SakuraStandStatus = ShowStatus("Unoperational","06/05/2025");

local MurderMystery2Status = ShowStatus("Unoperational","05/04/2025");

local AriseCrossoverStatus = ShowStatus("Unoperational","29/05/2025");

local ProjectMenacingStatus = ShowStatus("Unoperational","29/01/2025");

local StandsAwakeningStatus = ShowStatus("Unoperational","23/12/2024");

local SecondPieceStatus = ShowStatus("Unoperational","19/04/2025");

local UntitledGymGameStatus = ShowStatus("Unoperational","29/01/2025");

local AUniversalTimeStatus = ShowStatus("Unoperational","01/05/2025");

