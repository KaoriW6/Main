-- // Return Same Position \\ --
local cloneref = cloneref or function(o) return o end;
(getgenv()).SavePosition=function()local c="KaoriFolder/Assets/PositionManager/"local k=cloneref(game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"))if k then if not isfolder("KaoriFolder")then makefolder("KaoriFolder")end if not isfolder("KaoriFolder/Assets")then makefolder("KaoriFolder/Assets")end if not isfolder("KaoriFolder/Assets/PositionManager")then makefolder("KaoriFolder/Assets/PositionManager")end if not isfolder(c)then makefolder(c)end local r=c..("/Return - "..game.PlaceId)local o={X=k.Position.X,Y=k.Position.Y;Z=k.Position.Z}writefile(r,(game:GetService("HttpService")):JSONEncode(o))end end;(getgenv()).LoadPosition=function(c)return function(k)local r="KaoriFolder/Assets/PositionManager/"local o=r..("/Return - "..game.PlaceId)if isfile(o)then local r=(game:GetService("HttpService")):JSONDecode(readfile(o))local i=cloneref(game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"))if i then if c=="Teleport"then i.CFrame=CFrame.new(Vector3.new(r.X,r.Y,r.Z))task.spawn(function()(getgenv()).Debug("\240\159\159\162"..(string.rep(" ",3).."Loaded Previous Saved Position!"),1.5)end)elseif c=="Tween"then local c=Vector3.new(r.X,r.Y,r.Z)local o=(i.Position-c).Magnitude local j=tonumber(k)or 50 local t=o/j local E=TweenInfo.new(t,Enum.EasingStyle.Linear,Enum.EasingDirection.InOut)local b={CFrame=CFrame.new(c)}local U=(game:GetService("TweenService")):Create(i,E,b)U:Play()U.Completed:Connect(function()task.spawn(function()(getgenv()).Debug("\240\159\159\162"..(string.rep(" ",3).."Loaded Previous Saved Position!"),1.5)end)end)end end else task.spawn(function()(getgenv()).Debug("You cannot load yet because you didn't save a position in this game.",4)end)end end end
